<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow JIT (Just In Time) compilation for the Java VM -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>

    <!-- Allow unsigned memory execution for the Java VM -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>

    <!-- Allow DYLD environment variables, which may be necessary for debugging -->
    <key>com.apple.security.cs.allow-dyld-environment-variables</key>
    <true/>

    <!-- Disable library validation to allow loading of non-system libraries; use with caution -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>

    <!-- Networking entitlements for internet access -->
    <key>com.apple.security.network.client</key>
    <true/>

    <!-- File access entitlements for broad filesystem access, adjust if more specific paths are required -->
    <key>com.apple.security.files.user-selected.read-write</key>
    <true/>
    <key>com.apple.security.files.downloads.read-write</key>
    <true/>
    <key>com.apple.security.files.documents.read-write</key>
    <true/>

    <!-- Access to system administration features for printer management -->
    <key>com.apple.security.device.usb</key>
    <true/>
    <key>com.apple.security.print</key>
    <true/>

    <!-- Application needs to store data in user preferences -->
    <key>com.apple.security.application-groups</key>
    <array>
        <string>$(TeamIdentifierPrefix)com.connerum.modernprice</string>
    </array>
</dict>
</plist>